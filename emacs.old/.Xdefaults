#ifdef COLOR
*customization: -color
#endif

emacs*Background: DarkSlateGray
emacs*Foreground: Wheat
emacs*pointerColor: GreenYellow
emacs*cursorColor: Yellow
emacs*bitmapIcon: on
emacs*font: -adobe-courier-bold-r-normal--12-120-75-75-m-70-iso8859-1
emacs.geometry: 80x25

Seyon.modems: /dev/modem

xterm*background: Black
xterm*foreground: Wheat
xterm*cursorColor: Orchid
xterm*reverseVideo: false
xterm*scrollBar: true
xterm*reverseWrap: true
xterm*font: -misc-fixed-medium-r-normal--10-100-75-75-c-60-iso8859-1
xterm*fullCursor: true
xterm*scrollTtyOutput: off
xterm*scrollKey: on
;xterm*VT100.Translations: #override\n\
;	<KeyPress>Prior : scroll-back(1,page)\n\
;	<KeyPress>Next : scroll-forw(1,page)
xterm*titleBar: false

